{
  "cli": {
    "version": ">= 5.9.1",
    "requireCommit": "true",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "env": {
          "GOOGLE_SERVICES_IOS": "googleServicesFileBase64"
        }
      }
    },
    "production": {
      "ios": {
        "env": {
          "GOOGLE_SERVICES_IOS": "googleServicesFileBase64"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}